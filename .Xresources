#
#URxvt
#
# Extensions
URxvt*perl-ext-common: default,matcher,vtwheel,clipboard
#
# Looks
URxvt*transparent: true
URxvt*shading: 25
URxvt*scrollBar: false
URxvt*background:#00000080 
URxvt.foreground: #ffffff
URxvt.font: xft:DejavuSansMono:size=12
#
# Clickable urls
URxvt.url-select.underline: true
URxvt.url-launcher: /usr/bin/firefox
URxvt.matcher.button: 1
#
# Change keybinding for copy/paste
URxvt*iso14755: false
URxvt.keysym.Shift-Control-C: perl:clipboard:copy
URxvt.keysym.Shift-Control-V: perl:clipboard:paste
URxvt.letterSpace: -1
URxvt.lineSpace: 2
#
# Rofi
#
# Enabled modi
rofi.modi: ssh,window,drun,run
rofi.combi: modi
# Window width
# rofi.width: 50
# Number of lines
# rofi.lines: 15
# Number of columns
# rofi.columns: 1
# Font to use
# rofi.font: mono 12
# Border width
# rofi.bw: 0
# Location on screen
# rofi.location: 0
# Padding
# rofi.padding: 5
# Y-offset relative to location: Default "0"
# rofi.yoffset: 0
# X-offset relative to location: Default "0"
# rofi.xoffset: 0
# Always show number of lines: Default "true"
# rofi.fixed-num-lines: true
# Whether to load and show icons
rofi.show-icons: true
# Terminal to use
rofi.terminal: uxrvt
# Ssh client to use
rofi.ssh-client: ssh
# Ssh command to execute
rofi.ssh-command: {terminal} -e "{ssh-client} {host}"
# Run command to execute
rofi.run-command: {cmd}
# Command to get extra run targets: Default ""
# rofi.run-list-command:               
# Run command to execute that runs in shell
rofi.run-shell-command: {terminal} -e {cmd}
# Command executed on accep-entry-custom for window modus
rofi.window-command: xkill -id {window}
# Window fields to match in window mode: Default "all"
rofi.window-match-fields: all
# Theme to use to look for icons
rofi.icon-theme: Adwaita
# Desktop entry fields to match in drun: Default "name,generic,exec,categories"
# rofi.drun-match-fields: name,generic,exec,categories
# Desktop entry show actions: Default "false"
# rofi.drun-show-actions: false
# DRUN format string. (Supports: generic,name,comment,exec,categories): Default "{name} [<span weight='light' size='small'><i>({generic})</i></span>]"
# rofi.drun-display-format: {name} [<span weight='light' size='small'><i>({generic})</i></span>]
# Disable history in run/ssh
rofi.disable-history: false
